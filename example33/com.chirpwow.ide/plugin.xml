<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.chirpwow.ide.commands.category"
            name="Greeting">
      </category>
      <command
            categoryId="com.chirpwow.ide.commands.category"
            name="Greeting"
            id="com.chirpwow.ide.commands.greetingCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.chirpwow.ide.handlers.GreetingHandler"
            commandId="com.chirpwow.ide.commands.greetingCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.chirpwow.ide.commands.greetingCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="com.chirpwow.ide.menus.modelMenu"
               label="Model"
               mnemonic="M">
            <command
                  commandId="com.chirpwow.ide.commands.greetingCommand"
                  icon="icons/globe.png"
                  id="com.chirpwow.ide.menus.greetingCommand"
                  mnemonic="S"
                  tooltip="Greeting from ChirpWow">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.chirpwow.ide.toolbars.greetingToolbar">
            <command
                  id="com.chirpwow.ide.toolbars.greetingCommand"
                  commandId="com.chirpwow.ide.commands.greetingCommand"
                  icon="icons/globe.png"
                  tooltip="Greeting from ChirpWow">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
